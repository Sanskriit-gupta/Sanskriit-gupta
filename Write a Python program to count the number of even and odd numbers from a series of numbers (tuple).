
t=(1,2,3,4,5,6,7,8,9)
sum1=0
sum2=0
print("even:",end=" ")
for i in t:
    if i% 2==0:
        print(i,end=" ")
        sum1=sum1+(1/i)
        print("\n total even no. are: ",sum1)
        print()
        print("add:",end=" ")
        sum2=sum2+(1/i)
        print("\n total odd no. are : ",sum2)
        print()
